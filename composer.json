{
    "name": "alexjustesen/filament-spatie-laravel-activitylog",
    "description": "View your activity logs inside of Filament.",
    "keywords": [
        "alexjustesen",
        "laravel",
        "filament-spatie-laravel-activitylog"
    ],
    "homepage": "https://github.com/alexjustesen/filament-spatie-laravel-activitylog",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Justesen",
            "email": "support@alexjustesen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "filament/filament": "^2.7",
        "illuminate/contracts": "^8.0 || ^9.0 || ^10.0",
        "spatie/laravel-activitylog": "^4.3",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10 || ^6.0",
        "nunomaduro/larastan": "^1.0 || ^2.0.1",
        "orchestra/testbench": "^6.22 || ^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "AlexJustesen\\FilamentSpatieLaravelActivitylog\\": "src",
            "AlexJustesen\\FilamentSpatieLaravelActivitylog\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexJustesen\\FilamentSpatieLaravelActivitylog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexJustesen\\FilamentSpatieLaravelActivitylog\\FilamentSpatieLaravelActivitylogServiceProvider"
            ],
            "aliases": {
                "FilamentSpatieLaravelActivitylog": "AlexJustesen\\FilamentSpatieLaravelActivitylog\\Facades\\FilamentSpatieLaravelActivitylog"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
